<core:FragmentDefinition xmlns:l="sap.ui.layout" xmlns:f="sap.ui.layout.form" xmlns:core="sap.ui.core" xmlns="sap.m">
	<l:Grid id="detailFragment" defaultSpan="L12 M12 S12" width="auto">
		<l:content>
			<f:SimpleForm minWidth="1024" maxContainerCols="2" editable="false" layout="ResponsiveGridLayout" title="ToSupplier" labelSpanL="3"
				labelSpanM="3" emptySpanL="4" emptySpanM="4" columnsL="1" columnsM="1">
				<f:content>
					<Label text="EmailAddress"/>
					<Text text="{EmailAddress}"/>
					<Label text="PhoneNumber"/>
					<Text text="{PhoneNumber}"/>
					<Label text="WebAddress"/>
					<Text text="{WebAddress}"/>
				</f:content>
			</f:SimpleForm>
		</l:content>
	</l:Grid>
</core:FragmentDefinition>